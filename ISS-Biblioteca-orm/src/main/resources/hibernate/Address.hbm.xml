<hibernate-mapping>
    <class name="iss.library.libraryiss1.model.Address" table="addresses">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="streetName"
                  column="street_name"
                  type="string"/>
        <property name="streetNumber"
                  column="street_number"
                  type="integer"/>
        <property name="county"
                  column="county"
                  type="string"/>
  </class>
</hibernate-mapping>